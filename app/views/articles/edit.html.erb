<h1>Edit</h1>
<% if user_signed_in? || :protected != true %>
<%= form_for @article do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
<% if user_signed_in? %>
  <p>
    <%= f.check_box :protected %> Protected
  </p>
<% end %> 
  <p>
    <%= f.submit %>
  </p>
<% end %>
<% end %>